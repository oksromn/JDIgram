<div class="container">
  <div class="row">
    <h3>Users</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>

            <p>nickname:<%= user.nickname %></p>
            <p>firstname:<%= user.firstname %></p>
            <%= render user %>

          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
